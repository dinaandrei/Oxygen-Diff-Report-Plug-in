<smartphones>

    <smartphone id="001">
        <manufacturer>Apple</manufacturer>
        <model>iPhone SE</model>
        <cpu>
            <name>A4</name>
            <frequency unit="Ghz">1.3</frequency>
            <numberOfCores>2</numberOfCores>
        </cpu>
        <memory>
            <ram>
                <storage unit="GB">1</storage>
                <frequency unit="Mhz">800</frequency>
            </ram>
            <flash unit="GB">6</flash>
        </memory>
        <screen>
            <type>retina display</type>
            <diagonal unit="inch">4</diagonal>
            <resolution>
                <length unit="px">1920</length>
                <width unit="px">1080</width>
            </resolution>
        </screen>
        <camera>
            <frontFacingCamera unit="Mpx">5</frontFacingCamera>
            <rearCamera unit="Mpx">12</rearCamera>
        </camera>
        <conectivity>
            <bluetooth>true</bluetooth>
            <wifi>true</wifi>
            <_4g>false</_4g>
        </conectivity>
        <otherSpecs>barometer</otherSpecs>
        <otherSpecs>gyroscope</otherSpecs>
        <price unit="$">100</price>
    </smartphone>

